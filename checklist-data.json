            "map",
            "map-lite"
          ],
          "items": [
            {
              "id": "4-11",
              "text": "Identify modernization opportunities",
              "tooltip": "Assess migrated workloads for optimization and modernization",
              "links": []
            },
            {
              "id": "4-12",
              "text": "Implement cost optimization measures",
              "tooltip": "Right-size instances, implement auto-scaling, use Reserved Instances",
              "links": []
            },
            {
              "id": "4-13",
              "text": "Modernize applications where applicable",
              "tooltip": "Refactor to use cloud-native services like Lambda, containers, or managed databases",
              "links": []
            }
          ]
        }
