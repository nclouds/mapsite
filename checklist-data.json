  "phases": [
    {
      "id": "phase-1",
      "title": "Phase 1: Project Initiation & Eligibility",
      "sections": [
        {
          "title": "Both MAP & MAP Lite",
          "applicability": [
            "map",
            "map-lite"
          ],
          "items": [
            {
              "id": "1-1",
              "text": "Determine migration eligibility for MAP funding",
              "tooltip": "Verify that your migration project meets AWS MAP requirements including minimum ARR thresholds and partner eligibility criteria",
              "links": [],
              "required": true
            },
            {
              "id": "1-2",
              "text": "Confirm partner eligibility requirements are met",
              "tooltip": "Must have AWS Migration Competency or qualifying AWS Competencies/Service Delivery designations",
              "links": [
                {
                  "text": "Partner Eligibility Guide",
                  "url": "https://aws.amazon.com/partners/programs/migration-acceleration-program/"
                }
              ],
              "required": true
            },
            {
              "id": "1-3",
              "text": "Identify customer's anticipated post-migration Annual Run Rate (ARR)",
              "tooltip": "Calculate expected AWS spend after migration completion. MAP: $500K-$10M ARR, MAP Lite: $100K-$500K ARR",
              "links": [],
              "required": true
            },
            {
              "id": "1-4",
              "text": "Verify customer has not previously received MAP funding",
              "tooltip": "Customers can only receive MAP funding once. Check with AWS team if unsure",
              "links": [],
              "required": true
            },
            {
              "id": "1-5",
              "text": "Confirm migration timeline aligns with MAP requirements",
              "tooltip": "Migration must be completed within the program timeframe (typically 12-24 months)",
              "links": []
            }
          ]
        },
        {
          "title": "MAP Only ($500K-$10M ARR)",
          "applicability": [
            "map"
          ],
          "items": [
            {
              "id": "1-6",
              "text": "Complete Migration Readiness Assessment (MRA)",
              "tooltip": "The MRA evaluates your organization's readiness across people, process, and technology dimensions",
              "links": [
                {
                  "text": "MRA Guide",
                  "url": "https://docs.aws.amazon.com/prescriptive-guidance/latest/migration-readiness/welcome.html"
                }
              ],
              "required": true
            },
            {
              "id": "1-7",
              "text": "Document MRA findings and recommendations",
              "tooltip": "Create comprehensive report of gaps identified and remediation plans",
              "links": [],
              "required": true
            },
            {
              "id": "1-8",
              "text": "Share MRA results with customer stakeholders",
              "tooltip": "Present findings to executive sponsors and technical teams",
              "links": []
            }
          ]
        },
        {
          "title": "MAP Lite Only ($100K-$500K ARR)",
          "applicability": [
            "map-lite"
          ],
          "items": [
            {
              "id": "1-9",
              "text": "Complete simplified readiness review",
              "tooltip": "MAP Lite requires a lighter readiness assessment focused on key migration requirements",
              "links": []
            },
            {
              "id": "1-10",
              "text": "Verify MAP Lite specific requirements",
              "tooltip": "Ensure project meets the streamlined requirements for MAP Lite funding",
              "links": []
            }
          ]
        }
      ]
    },
    {
      "id": "phase-2",
