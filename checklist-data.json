    {
      "id": "phase-6",
      "title": "Phase 6: Compliance & Best Practices",
      "sections": [
        {
          "title": "Program Compliance",
          "applicability": [
            "map",
            "map-lite"
          ],
          "items": [
            {
              "id": "6-1",
              "text": "Ensure only MAP-eligible services are included",
              "tooltip": "Review MAP Included Services List to verify all services are eligible for funding",
              "links": [
                {
                  "text": "MAP Included Services",
                  "url": "https://aws.amazon.com/migration-acceleration-program/included-services"
                }
              ],
              "required": true
            },
            {
              "id": "6-2",
              "text": "Maintain accurate project documentation",
              "tooltip": "Keep all project artifacts up-to-date for potential AWS audits",
              "links": [],
              "required": true
            },
            {
              "id": "6-3",
              "text": "Follow AWS Well-Architected Framework",
              "tooltip": "Ensure migrations align with AWS best practices for security, reliability, and cost optimization",
              "links": [
                {
                  "text": "Well-Architected",
                  "url": "https://aws.amazon.com/architecture/well-architected/"
                }
              ]
            },
            {
              "id": "6-4",
              "text": "Implement proper cost allocation tags",
              "tooltip": "Use consistent tagging strategy for cost tracking and optimization",
              "links": [],
              "required": true
            },
            {
              "id": "6-5",
              "text": "Maintain security and compliance standards",
              "tooltip": "Ensure all migrated workloads meet security and compliance requirements",
              "links": []
            }
          ]
        },
        {
          "title": "Common Pitfalls to Avoid",
          "applicability": [
            "map",
            "map-lite"
          ],
          "items": [
            {
              "id": "6-6",
              "text": "Verify workloads are net-new to AWS",
              "tooltip": "MAP funding only applies to workloads not previously running on AWS",
              "links": [],
              "required": true
            },
            {
              "id": "6-7",
              "text": "Avoid including ineligible services in claims",
              "tooltip": "Services like Marketplace, Support plans, and certain data transfer costs are not eligible",
              "links": []
            },
            {
              "id": "6-8",
              "text": "Submit claims within program timelines",
              "tooltip": "Claims must be submitted within the approved project timeline",
              "links": [],
              "required": true
            },
            {
              "id": "6-9",
              "text": "Maintain clear audit trail",
              "tooltip": "Keep detailed records of all migration activities and spending",
              "links": []
            }
          ]
        }
      ]
